---
eleventyExcludeFromCollections: true
---
<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>{{ site.name }}</title>
    <link rel="canonical" href="{{ site.url }}">
  </head>
  <body>
    <header>
      <h1>{{ site.name }}</h1>
    </header>
    <main role="main">
      <nav>
        <ul>
          {%- for post in collections.post | reverse %}
          <li>
            <a href="{{ post.url }}">
              <h2>{{ post.data.title | escape }}</h2>
            </a>
          </li>
          {%- endfor %}
        </ul>
      </nav>
    </main>
    <footer>
      <small><a href="{{ site.url }}">{{ site.name }}</a> &copy; 2022 sasa+1</small>
    </footer>
  </body>
</html>
